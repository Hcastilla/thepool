{
  "name": "The_Pool",
  "version": "0.0.1",
  "description": "Simple boilerplate para weback y webpack-dev-server",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "client:watch": "webpack --watch --config ./webpack.config.client.js",
    "client:server": "webpack-dev-server --config ./webpack.config.client.js",
    "server:webpack": "webpack --watch --config ./webpack.config.server.js",
    "server:watch": "nodemon  exec babel ./bin/dev",
    "server:dev": "npm-run-all --parallel server:watch client:watch"
  },
  "keywords": [
    "boilerplate",
    "webpack",
    "webpack-dev-server",
    "webpack configuration"
  ],
  "author": "Hernan Castilla",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/register": "^7.4.4",
    "@material-ui/core": "^4.1.0",
    "@material-ui/icons": "^4.1.0",
    "@material-ui/styles": "^4.1.1",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "css-loader": "^3.0.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "hapi": "^18.1.0",
    "inert": "^5.1.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "material-ui-search-bar": "^0.4.2",
    "mini-css-extract-plugin": "^0.7.0",
    "moment": "^2.24.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-moment": "^0.9.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "rxjs": "^6.5.2",
    "rxjs-compat": "^6.5.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "typescript": "^3.5.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "validator": "^11.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.4.4"
  }
}
