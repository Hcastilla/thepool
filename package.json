{
  "name": "webpack-boirtemplate",
  "version": "1.0.0",
  "description": "Simple boilerplate para weback y webpack-dev-server",
  "main": "src/js/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "client:watch": "webpack --watch",
